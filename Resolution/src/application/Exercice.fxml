<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="305.0" prefWidth="386.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreateExerciceController">
   <children>
      <AnchorPane prefHeight="359.0" prefWidth="529.0">
         <children>
            <TabPane fx:id="Tb" layoutY="-1.0" prefHeight="381.0" prefWidth="554.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Etudiant">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="347.0" prefWidth="542.0">
                           <children>
                              <TextField fx:id="textTry" layoutX="59.0" layoutY="266.0" />
                              <Button layoutX="245.0" layoutY="266.0" mnemonicParsing="false" onAction="#ShowString" text="Vérifier" />
                              <TextArea fx:id="containerEleve" layoutX="58.0" layoutY="43.0" prefHeight="200.0" prefWidth="427.0" />
                           </children></AnchorPane>
                  </content>
                </Tab>
                <Tab text="Prof">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TextArea fx:id="containerProf" layoutX="27.0" layoutY="27.0" prefHeight="191.0" prefWidth="476.0" promptText="Saisisez votre texte..." />
                              <Button fx:id="create" layoutX="243.0" layoutY="263.0" mnemonicParsing="false" onAction="#CreateExercice" text="Créer" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                  <Tab text="Video">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="347.0" prefWidth="542.0">
                           <children>
                              <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" layoutX="173.0" layoutY="68.0" />
                              <Button fx:id="play" layoutX="34.0" layoutY="98.0" mnemonicParsing="false" onAction="#play" prefHeight="26.0" prefWidth="49.0" text="Lire" />
                              <Button fx:id="pause" layoutX="34.0" layoutY="98.0" mnemonicParsing="false" onAction="#pause" text="Pause" visible="false" />
                              <Button layoutX="34.0" layoutY="143.0" mnemonicParsing="false" onAction="#stop" text="Stop" />
                              <Slider fx:id="volume" layoutX="5.0" layoutY="195.0" />
                              <Button layoutX="32.0" layoutY="243.0" mnemonicParsing="false" onAction="#browser" text="Choisir..." />
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
